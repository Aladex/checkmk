{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug test_cmc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/enterprise/core/src/test_cmc", //"${workspaceFolder}/main.exe",
            "args": [
                "--gtest_filter=GenericCheckHelperPoolFixture.TestPool*"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/enterprise/core/src",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "Build Core Test",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "GDB Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/bubble_sort_cunit",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}